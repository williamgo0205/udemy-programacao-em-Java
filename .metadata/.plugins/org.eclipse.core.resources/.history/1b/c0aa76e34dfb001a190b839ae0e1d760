package br.com.geekuniversity.secao12;

public class Aluno {
	private String ra;
	private Pessoa pessoa;
	
	public Aluno(String ra, Pessoa pessoa) {
		this.ra = ra;
		this.pessoa = pessoa;
	}
	
	public String getRa() {
		return ra;
	}
	
	public void setRA(String ra) {
		ra = ra;
	}
	
	public Pessoa getPessoa() {
		return pessoa;
	}
	
	public void setPessoa(Pessoa pessoa) {
		this.pessoa = pessoa;
	}
	
	


}
