package br.com.geekuniversity.secao11;

public class Cliente {
	private String nome;
	private String endereco;
	
	public Cliente(String nome, String endereco) {
		super();
		this.nome = nome;
		this.endereco = endereco;
		
		this.dizer_oi();
	}
		
	private void dizer_oi() {
		System.out.println("Oi...");
	}
	
}
