import java.util.ArrayList;
import java.util.List;

// Vetores parte 1
// Arrays (Vetores Uni-dimencionais)

/*
 * float valores[10]; //0..9
 int numeros[5];//0..4 (n-1) 5-1 =4
 
 // Veror[indice]
 numeros[0] = 1;
 numeros[1] = 3;
 numeros[2] = 5;
 numeros[3] = 7;
 numeros[4] = 9;
  
 */
public class Programa16 {

	public static void main(String[] args) {
		int numeros[] = new int[5];
		
		numeros[0] = 1;
		numeros[1] = 3;
		numeros[2] = 5;
		numeros[3] = 7;
		numeros[4] = 9;
		
		for (int i : numeros) {
			System.out.println("Número na posição [ " + i + "] é :" + numeros[i]);
		}
	}

}
