// If, else, if else
public class Programa04 {

	public static void main(String[] args) {
		
		int numero = 3;

		if (numero > 5) {
			System.out.println("Sim, o número " + numero + " é maior que 5");
		}else {
			System.out.println("Não, o número " + numero + " não é maior que 5");
		}
		
	}

}
