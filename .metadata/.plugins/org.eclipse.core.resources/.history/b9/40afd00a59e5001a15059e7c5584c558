// Operações Matemáticas
public class Programa15 {

	public static void main(String[] args) {
		int num1 = 5, num2 = 9, res;
		
		//soma
		res = num1 + num2;
		System.out.println("A Soma de " + num1 + " + " + num2 + " = " + res);
		
		//subtração
		res = num2 - num1;
		System.out.println("A Subtração de " + num2 + " - " + num1 + " = " + res);
		
		//Multiplicação
		res = num1 * num2;
		System.out.println("A Multiplicação de " + num1 + " *  " + num2 + " = " + res);
		
		//Divisão
		res = num2 / num1;
		System.out.println("A Divisão de " + num2 + " *  " + num1 + " = " + res);
	}

}
